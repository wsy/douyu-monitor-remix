{
  "name": "remix-template-remix-ts",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "concurrently \"npm run sass\" \"remix build\"",
    "dev": "concurrently \"npm run sass:dev\" \"remix dev\"",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "sass:dev": "sass --watch --no-source-map --style compressed app/:app/",
    "sass": "sass --no-source-map --style compressed app/:app/"
  },
  "dependencies": {
    "@remix-run/react": "^1.3.3",
    "@remix-run/serve": "^1.3.3",
    "@vant/touch-emulator": "^1.3.2",
    "clsx": "^1.1.1",
    "copy-to-clipboard": "^3.3.1",
    "immer": "^9.0.12",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-vant": "2.0.0-alpha.26",
    "remix": "^1.3.3",
    "use-immer": "^0.6.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.3.3",
    "@remix-run/eslint-config": "^1.3.3",
    "@types/react": "^17.0.42",
    "@types/react-dom": "^17.0.9",
    "concurrently": "^7.0.0",
    "eslint": "^8.11.0",
    "sass": "^1.49.9",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  }
}
